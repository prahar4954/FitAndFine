<h1 class="pb-4">Login</h1>
<p class="grey-text text-darken-1">
  Don't have an account? <a href="/register">Register</a>
</p>
      <form noValidate action="/login" method="POST">
        <div class="input-field col s12">
          <label htmlFor="email">Email</label>
          <input
            id="email"
            name="email"
            type="email"
          />
          
        </div>
        <div class="input-field col s12">
          <label htmlFor="password">Password</label>
          <input
            id="password"
            name="password"
            type="password"
          />
          
        </div>
        <div class="col s12" style="padding-left:11.250px">
          <button
          style="
          width: 150px;
          border-radius: 3px;
          letter-spacing: 1.5px;
          margin-top: 1rem"
            type="submit"
            class="btn btn-dark"
          >
            Login
          </button>
        </div>
      </form>
</div>